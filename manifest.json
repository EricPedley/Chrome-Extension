{
    "name":"DeepCtrlF",
    "version":"1.0",
    "description":"ctrl f that also searches links in webpage",
    "background": {
        "scripts":["background.js"],
        "persistent":true
    },
    "permissions": [
        "https://*/*",
    "http://*/*",
        "contextMenus",
        "storage",
        "declarativeContent",
        "activeTab"
    ],
      "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
      },
      "content_scripts": [
        {
          "matches": ["http://*/*","https://*/*"],
          "js":["jquery.min.js","content.js"]
        }
      ],
      "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "manifest_version":2

}